{
  "name": "example-simple",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "VUE_APP_PRODUCTION=false vue-cli-service serve",
    "build": "vue-cli-service build",
    "rules": "npm run rules:make && npm run rules:compile && npm run rules:deploy",
    "lint": "vue-cli-service lint",
    "rules:make": "helios rules --write database.rules.bolt",
    "rules:compile": "firebase-bolt database.rules.bolt",
    "rules:deploy": "firebase deploy --only database",
    "lint:schema": "helios check db/rules/check_schema.js"
  },
  "dependencies": {
    "core-js": "^3.4.3",
    "firebase": "^7.4.0",
    "heliosrx": "file:../..",
    "moment": "^2.24.0",
    "vue": "^2.6.10",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
